{
    "extends": "airbnb-base",
    "env": {
        "browser": true
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "eol-last": 0,
        "padded-blocks": [
            "error",
            "always"
        ],
        "comma-dangle": [
            "error",
            "never"
        ]
    }
}