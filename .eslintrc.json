{
    "extends": "airbnb-base",
    "env": {
        "browser": true
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "eol-last": 0,
        "padded-blocks": [
            {
                "blocks": "never",
                "classes": "always",
                "switches": "never"
            }
        ],
        "comma-dangle": [
            "error",
            "never"
        ]
    }
}